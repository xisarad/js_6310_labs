{
  "manifest_version": 3,
  "name": "KAI Style Changer",
  "version": "1.0",
  "description": "Изменяет стиль сайта kai.ru",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://kai.ru/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"]
}